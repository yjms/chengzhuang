/* pages/distribution/team/team.wxss */
page{
  width: 100%;
  height: 100%;
}
.warp{
  width: 100%;
  height: 100%;
  background: #EEEEEE;
}
.list{
  padding-top: 30rpx;
  background: #fff;
}
.list .li {
  display: flex;
  flex-direction: row;
  padding-bottom: 26rpx;
}
.list .li:last-child{
  padding-bottom: 0;
}
.list .li .l1{
  width: 139rpx;
  text-align: center;
}
.list .li .l1 image{
  width: 70rpx;
  height: 70rpx;
  background: #999999;
  border-radius: 50%;
}
.list .li .l2{
  flex: 1;
  display: flex;
  flex-direction: row;
  border-bottom: 1px solid #EEEEEE;
  padding-bottom: 26rpx;
}
.list .li:last-child .l2{
  border-bottom: none;
}
.list .li .l2 .l2_1{
  width: 33.3%;
  text-align: center;
}
.list .li .l2 .l2_1:first-child{
  text-align: left;
}
.list .li .l2 .l2_1 .text1{
  font-size: 26rpx;
  color: #4B5248;
}
.list .li .l2 .l2_1 .text2{
  font-size: 24rpx;
  color: #A8ADA6;
}
.tip{
  text-align: center;
  padding-top: 20rpx;
}
.tip image{
  width: 290rpx;
}