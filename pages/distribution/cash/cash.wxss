/* pages/distribution/cash/cash.wxss */
page{
  width: 100%;
  height: 100%;
}
.warp{
  width: 100%;
  height: 100%;
  background: #EEEEEE;
}
.header{
  width: 750rpx;
  height: 80rpx;
  line-height: 80rpx;
  background: #8D9789;
  color: #fff;
  display: flex;
  flex-direction: row;
}
.header .h1{
  flex: 1;
  text-align: center;
}
.header .h1:first-child{
  text-align: left;
  padding-left: 50rpx;
}
.list{
  padding-left: 50rpx;
  box-sizing: border-box;
  background: #fff;
}
.flex{
  width: 700rpx;
  height: 102rpx;
  line-height: 102rpx;
  display: flex;
  flex-direction: row;
  border-bottom: 1px solid #eeeeee;
}
.flex .f1{
  flex: 1;
  font-size: 26rpx;
  color: #4B5248;
}
.flex .f2{
  flex: 1;
  font-size: 24rpx;
  color: #555555;
  text-align: center;
}
.flex .f3{
  flex: 1;
  font-size: 24rpx;
  color: #555555;
  text-align: center;
}
.tip{
  text-align: center;
  padding-top: 20rpx;
}
.tip image{
  width: 290rpx;
}