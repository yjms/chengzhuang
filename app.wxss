/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 

.nowrap{
  white-space: nowrap; 
  overflow: hidden;
  text-overflow:ellipsis;
}
.setFlex{
	flex: 1;
	box-sizing: border-box;
}
.number{
	min-width: 20rpx !important;
	min-height: 20rpx !important;
}